{
  "name": "complens-frontend",
  "version": "1.0.0",
  "description": "Complens.ai - AI-powered application frontend",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "axios": "^1.6.0",
    "react-router-dom": "^6.20.0",
    "amazon-cognito-identity-js": "^6.3.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && aws s3 sync build/ s3://${FRONTEND_BUCKET} --delete && aws cloudfront create-invalidation --distribution-id ${CLOUDFRONT_ID} --paths '/*'"
