# Cognito configuration
VITE_COGNITO_USER_POOL_ID=us-east-1_XXXXX
VITE_COGNITO_CLIENT_ID=your-client-id-here

# API URL (optional - defaults to /api in dev)
VITE_API_URL=https://api.dev.complens.ai

# Subdomain suffix for page subdomains (e.g., mypage.dev.complens.ai)
# Defaults to stripping "api." from VITE_API_URL if not set
VITE_SUBDOMAIN_SUFFIX=dev.complens.ai
